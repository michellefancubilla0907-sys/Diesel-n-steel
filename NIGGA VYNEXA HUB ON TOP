local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()

local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({

    Title = "VYNEXA HUB",

    SubTitle = "by eiron",

    TabWidth = 160,

    Size = UDim2.fromOffset(600, 460),

    Acrylic = true,

    Theme = "Dark",

    MinimizeKey = Enum.KeyCode.LeftControl

})

local Tabs = {

    Main = Window:AddTab({ Title = "Main", Icon = "terminal" }),

    Farm = Window:AddTab({ Title = "Auto Farm", Icon = "activity" }),

    Teleport = Window:AddTab({ Title = "Teleport", Icon = "map" }),

    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })

}

local Options = Fluent.Options

local RunCode = ""

local RunLoop = false

Tabs.Main:AddInput("RunInput", {

    Title = "Lua Code Runner",

    Default = "",

    Placeholder = "-- Enter your Lua code here",

    Numeric = false,

    Finished = false,

    Callback = function(Value)

        RunCode = Value

    end

})

Tabs.Main:AddToggle("RunLoop", { Title = "Run Loop", Default = false }):OnChanged(function(Value)

    RunLoop = Value

    if RunLoop then

        task.spawn(function()

            while RunLoop do

                pcall(function()

                    if RunCode ~= "" then

                        local f, err = loadstring(RunCode)

                        if f then

                            pcall(f)

                        end

                    end

                end)

                task.wait(0.01)

            end

        end)

    end

end)

Tabs.Farm:AddToggle("AutoCoin", { Title = "Auto Coin", Default = false }):OnChanged(function(Value)

    if Value then

        task.spawn(function()

            while Options.AutoCoin.Value do

                pcall(function()

                    local rs = game:GetService("ReplicatedStorage")

                    local ws = game:GetService("Workspace")

                    local rem = rs:WaitForChild("Remotes"):WaitForChild("RecieveCoin")

                    for _, v in pairs(ws:WaitForChild("Jeepnies"):GetChildren()) do

                        local pv = v:FindFirstChild("PassengerValues")

                        if pv then

                            rem:FireServer({

                                Value = 300,

                                PassengerValues = pv,

                                Password = 123456789

                            })

                        end

                    end

                end)

                task.wait(0.01)

            end

        end)

    end

end)

Tabs.Farm:AddToggle("AutoExp", { Title = "Auto Exp", Default = false }):OnChanged(function(Value)

    if Value then

        task.spawn(function()

            while Options.AutoExp.Value do

                pcall(function()

                    local player = game.Players.LocalPlayer

                    local exp = player:WaitForChild("leaderstats"):WaitForChild("Exp")

                    exp.Value = exp.Value + 3000

                end)

                task.wait(0.3)

            end

        end)

    end

end)

Tabs.Farm:AddToggle("AutoCash", { Title = "Auto Cash", Default = false }):OnChanged(function(Value)

    if Value then

        task.spawn(function()

            while Options.AutoCash.Value do

                pcall(function()

                    local rs = game:GetService("ReplicatedStorage")

                    local rem = rs:WaitForChild("Remotes"):WaitForChild("RecieveCash")

                    local args = {

                        [1] = {

                            Value = 100,

                            Main = true,

                            Password = 584898869

                        }

                    }

                    rem:FireServer(unpack(args))

                end)

                task.wait(0.3)

            end

        end)

    end

end)

local SelectedPlace = "Bulakan"

local TeleportDropdown = Tabs.Teleport:AddDropdown("TeleportList", {

    Title = "Select Location",

    Values = { "Bulakan", "Malolos", "Balagtas", "Guiguinto", "BulakanTerm", "BalagtasTerm", "MalolosTerm", "GuiguintoTerm" },

    Multi = false,

    Default = 1

})

TeleportDropdown:OnChanged(function(Value)

    SelectedPlace = Value

end)

Tabs.Teleport:AddButton({

    Title = "Teleport Now",

    Description = "Click to teleport to selected location",

    Callback = function()

        local locs = {

            Bulakan = Vector3.new(-1545, 13, -3470),

            Malolos = Vector3.new(17796, 13, -1104),

            Balagtas = Vector3.new(-3879, 14, 3482),

            Guiguinto = Vector3.new(822, 13, 3290),

            BulakanTerm = Vector3.new(-626, 16, -3202),

            BalagtasTerm = Vector3.new(-3922, 17, 3156),

            MalolosTerm = Vector3.new(17606, 16, -1195),

            GuiguintoTerm = Vector3.new(1060, 16, 3167)

        }

        local p = game.Players.LocalPlayer

        local char = p.Character or p.CharacterAdded:Wait()

        local hrp = char:FindFirstChild("HumanoidRootPart")

        if hrp and locs[SelectedPlace] then

            hrp.CFrame = CFrame.new(locs[SelectedPlace])

        end

    end

})

SaveManager:SetLibrary(Fluent)

InterfaceManager:SetLibrary(Fluent)

SaveManager:IgnoreThemeSettings()

SaveManager:SetIgnoreIndexes({})

InterfaceManager:SetFolder("Free_Vault")

SaveManager:SetFolder("Free_Vault/Configs")

InterfaceManager:BuildInterfaceSection(Tabs.Settings)

SaveManager:BuildConfigSection(Tabs.Settings)

Window:SelectTab(1)

Fluent:Notify({

    Title = "VYNEXA HUB",

    Content = "Loaded successfully. Welcome,by eiron!",

    Duration = 6

})

SaveManager:LoadAutoloadConfig()
